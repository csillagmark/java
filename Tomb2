import java.util.Scanner;
public class Tomb2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("adj meg egy sz√°mot: ");
        int szam = sc.nextInt();

        double[] t = new double[szam];

        for(int i = 0; i < szam; i++)
        {
            t[i] = sc.nextDouble();
        }
    }
}
