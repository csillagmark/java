import java.util.Scanner;
public class Tomb3 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        System.out.print("adj meg egy sz√°mot: ");
        int szam = sc.nextInt();

        String[] t = new String[szam];

        for(int i = 0; i < szam; i++)
        {
            t[i] = sc.next();
        }
    }
}
